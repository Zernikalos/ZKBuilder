{
  "name": "@zernikalos/zkbuilder",
  "version": "0.0.1",
  "license": "MPL 2.0",
  "author": "Aaron Negrin",
  "main": "dist/zkbuilder.js",
  "browser": "dist/zkbuilder.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "start": "webpack watch --mode=development",
    "build": "pnpm run generate && pnpm run webpack",
    "build:dev": "webpack --mode=development",
    "webpack": "webpack --mode=production",
    "build:profile": "webpack --env useMode=profile",
    "parse": "babel-node index.js",
    "generate": "pbjs --target static-module --wrap es6 --es6 --force-message --no-delimited --out protobuild/index.js proto/zko.proto && pbts --no-comments --out protobuild/index.d.ts protobuild/index.js"
  },
  "dependencies": {
    "@zernikalos/zernikalos": "../Zernikalos/build/js/packages/@zernikalos/zernikalos",
    "@babel/runtime": "7.23.2",
    "three": "0.158.0",
    "lodash": "4.17.21",
    "is-obj": "3.0.0",
    "is-regexp": "3.1.0",
    "get-own-enumerable-property-symbols": "3.0.2",
    "protobufjs": "7.2.5",
    "hash-it": "6.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.23.3",
    "@babel/core": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.3",
    "@babel/plugin-proposal-export-default-from": "7.23.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0",
    "webpack-bundle-analyzer": "4.9.1",
    "babel-loader": "9.1.3",
    "@types/lodash": "4.14.201",
    "@types/three": "0.158.2",
    "@types/object-hash": "3.0.6",
    "protobufjs-cli": "1.1.2",
    "ts-loader": "9.5.0",
    "typescript": "5.2.2"
  }
}
