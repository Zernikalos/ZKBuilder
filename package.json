{
  "name": "@zernikalos/zkbuilder",
  "version": "0.0.1",
  "license": "MPL 2.0",
  "author": "Aaron Negrin",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "exports": [
    "dist/index.js",
    "dist/runtime.js",
    "dist/vendor.js",
    "dist/zernikalos.js",
    "dist/zkoproto.js"
  ],
  "types": "./build/index.d.ts",
  "scripts": {
    "start": "webpack watch --mode=development",
    "build": "npm-run-all protobuild:prod",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build:profile": "webpack --env useMode=profile",
    "prebuild": "npm-run-all clean:build",
    "parse": "babel-node index.js",
    "generate": "run-s clean:proto proto:folder proto:generate",
    "proto:folder": "make-dir protobuild",
    "proto:generate": "run-s proto:generate:pbjs proto:generate:pbts",
    "proto:generate:pbjs": "pbjs --target static-module --wrap es6 --es6 --force-message --no-delimited --out protobuild/index.js proto/zko.proto",
    "proto:generate:pbts": "pbts --no-comments --out protobuild/index.d.ts protobuild/index.js",
    "clean": "run-s clean:proto clean:build clean:modules",
    "clean:proto": "rimraf protobuild",
    "clean:build": "rimraf dist build",
    "clean:modules": "rimraf node_modules"
  },
  "dependencies": {
    "@zernikalos/zernikalos": "../Zernikalos/build/js/packages/@zernikalos/zernikalos",
    "get-own-enumerable-property-symbols": "3.0.2",
    "hash-it": "6.0.0",
    "is-obj": "3.0.0",
    "is-regexp": "3.1.0",
    "lodash": "4.17.21",
    "protobufjs": "7.4.0",
    "three": "0.170.0"
  },
  "devDependencies": {
    "@types/lodash": "4.17.13",
    "@types/object-hash": "3.0.6",
    "@types/three": "0.169.0",
    "protobufjs-cli": "1.1.3",
    "ts-loader": "9.5.1",
    "typescript": "5.6.3",
    "webpack": "5.96.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-merge": "6.0.1",
    "make-dir-cli": "4.0.0",
    "rimraf": "6.0.1",
    "npm-run-all2": "7.0.1"
  }
}
