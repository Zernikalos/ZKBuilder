{
  "name": "@zernikalos/zkbuilder",
  "version": "0.0.1",
  "license": "MPL 2.0",
  "author": "Aaron Negrin",
  "main": "dist/zkbuilder.js",
  "browser": "dist/zkbuilder.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "start": "webpack watch --mode=development",
    "build": "pnpm run generate && pnpm run webpack",
    "build:dev": "webpack --mode=development",
    "webpack": "webpack --mode=production",
    "build:profile": "webpack --env useMode=profile",
    "parse": "babel-node index.js",
    "generate": "npm run generate:clean && npm run generate:folder && npm run generate:proto",
    "generate:clean": "rimraf protobuild",
    "generate:folder": "make-dir protobuild",
    "generate:proto": "pbjs --target static-module --wrap es6 --es6 --force-message --no-delimited --out protobuild/index.js proto/zko.proto && pbts --no-comments --out protobuild/index.d.ts protobuild/index.js"
  },
  "dependencies": {
    "@zernikalos/zernikalos": "../Zernikalos/build/js/packages/@zernikalos/zernikalos",
    "get-own-enumerable-property-symbols": "3.0.2",
    "hash-it": "6.0.0",
    "is-obj": "3.0.0",
    "is-regexp": "3.1.0",
    "lodash": "4.17.21",
    "protobufjs": "7.3.2",
    "three": "0.167.1"
  },
  "devDependencies": {
    "@types/lodash": "4.17.7",
    "@types/object-hash": "3.0.6",
    "@types/three": "0.167.2",
    "protobufjs-cli": "1.1.2",
    "ts-loader": "9.5.1",
    "typescript": "5.5.4",
    "webpack": "5.93.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-merge": "6.0.1",
    "make-dir-cli": "4.0.0",
    "rimraf": "6.0.1"
  }
}
