{
  "name": "@zernikalos/zkbuilder",
  "version": "0.3.0",
  "license": "MPL 2.0",
  "author": "Aaron Negrin",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zernikalos/ZKBuilder.git",
    "directory": "packages/zkbuilder"
  },
  "files": [
    "dist-node",
    "dist-browser",
    "build",
    "src",
    "README.md"
  ],
  "main": "dist-node/index.js",
  "browser": "dist-browser/index.js",
  "types": "./build/src/index.d.ts",
  "scripts": {
    "start": "webpack watch --mode=development",
    "build": "pnpm run build:browser && pnpm run build:node",
    "build:browser": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build:profile": "webpack --env useMode=profile",
    "build:node": "webpack --mode=development --target=node",
    "prebuild": "pnpm run clean:build",
    "parse": "babel-node index.js",
    "generate": "pnpm run clean:proto && pnpm run proto:folder && pnpm run proto:generate",
    "proto:folder": "make-dir protobuild",
    "proto:generate": "pnpm run proto:generate:pbjs && pnpm run proto:generate:pbts",
    "proto:generate:pbjs": "pbjs --target static-module --wrap es6 --es6 --force-message --no-delimited --out protobuild/index.js proto/zko.proto",
    "proto:generate:pbts": "pbts --no-comments --out protobuild/index.d.ts protobuild/index.js",
    "clean": "pnpm run clean:proto && pnpm run clean:build && pnpm run clean:modules",
    "clean:proto": "rimraf protobuild",
    "clean:build": "rimraf dist build dist-browser dist-node",
    "clean:modules": "rimraf node_modules"
  },
  "dependencies": {
    "@xmldom/xmldom": "0.9.8",
    "get-own-enumerable-property-symbols": "3.0.2",
    "is-obj": "3.0.0",
    "is-regexp": "3.1.0",
    "isbinaryfile": "5.0.7",
    "lodash": "4.17.21",
    "protobufjs": "7.5.4",
    "three": "0.181.1"
  },
  "devDependencies": {
    "@types/lodash": "4.17.20",
    "@types/node": "24.10.1",
    "@types/three": "0.181.0",
    "make-dir-cli": "4.0.0",
    "npm-run-all2": "8.0.4",
    "protobufjs-cli": "1.1.3",
    "rimraf": "6.1.0",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "webpack": "5.102.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "webpack-merge": "6.0.1"
  },
  "peerDependencies": {
    "@zernikalos/zernikalos": "0.6.0"
  }
}
